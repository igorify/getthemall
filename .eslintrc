{
	"extends": "airbnb-base",
	"rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"func-names": 0,
		"comma-dangle": 0,
		"no-restricted-syntax": 0,
		"no-await-in-loop": 0,
		"arrow-parens": 0,
		"no-return-await": 0
	},
	"parserOptions": {
		"ecmaVersion": 2017
	},
	"env": {
		"node": true,
		"mocha": true
	}
}
